package Recursion.Stack;

public class MyStack
{
	
	int capacity;
	Object stk[];
	int top;
	
	public MyStack(int capacity)
	{
		this.capacity=capacity;
		this.stk= new Object[capacity];
		top=-1;
	}

	public void push(Object obj)
	{
		if(top==capacity-1)
		{
			System.out.println("Stack overflow");
			return;
		}
		stk[++top]=obj;
		
	}

	
	
	
}





